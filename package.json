{
  "name": "sketch-chain",
  "private": true,
  "engines": {
    "node": "12.x"
  },
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c --watch",
    "serve": "COOKIE_SECRET=devonly nodemon --watch .data/dist/server .data/dist/server",
    "start": "node ./start.js",
    "watchnserve": "npm run build && (npm run watch & npm run serve)"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "11.1.0",
    "@rollup/plugin-node-resolve": "7.1.3",
    "@types/bluebird": "3.5.30",
    "@types/express": "4.17.6",
    "@types/express-session": "1.17.0",
    "@types/node": "13.13.4",
    "@types/session-file-store": "1.2.1",
    "@types/validator": "13.0.0",
    "@types/ws": "7.2.4",
    "cssnano": "4.1.10",
    "del": "5.1.0",
    "husky": "4.2.5",
    "lint-staged": "10.2.2",
    "nodemon": "2.0.3",
    "pointer-tracker": "2.3.0",
    "postcss": "7.0.28",
    "postcss-nested": "4.2.1",
    "postcss-url": "8.0.0",
    "prettier": "2.0.5",
    "rollup": "2.7.6",
    "rollup-plugin-terser": "5.3.0",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "express": "4.18.2",
    "express-async-handler": "1.1.4",
    "express-http-to-https": "1.1.4",
    "express-session": "1.17.1",
    "express-static-gzip": "2.0.6",
    "friendly-words": "1.1.10",
    "glob": "7.1.6",
    "google-auth-library": "5.10.1",
    "path-to-regexp": "6.1.0",
    "preact": "10.4.1",
    "preact-render-to-string": "5.1.6",
    "sequelize": "5.21.7",
    "session-file-store": "1.4.0",
    "simplify-js": "1.2.4",
    "sqlite3": "4.2.0",
    "ws": "7.2.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md,ts,tsx}": "prettier --write"
  }
}
